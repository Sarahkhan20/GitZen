{
       "name": "GitZen",
       "version": "1.0.0",
       "description": "GitZen is a powerful web-based tool designed to help users extract and summarize code from public GitHub repositories.",
       "main": "index.js",
       "scripts": {
              "build-client": "cd frontend && npm run build",
              "install-client": "cd frontend && npm install",
              "install-server": "cd server && npm install",
              "setup-production": "npm run install-client && npm run build-client && npm run install-server",
              "start-server": "cd server && node server.js",
              "start-client": "cd frontend && npm run start",
              "start-production": "npm run start-server",
              "postinstall": "cd frontend && npm install && cd ../server && npm install"
       },
       "repository": {
              "type": "git",
              "url": "git+https://github.com/Sarahkhan20/GitZen"
       },
       "keywords": [
              "github"
       ],
       "author": "Sarah Khan",
       "license": "MIT",
       "bugs": {
              "url": "https://github.com/Sarahkhan20/GitZen/issues"
       },
       "homepage": "https://github.com/Sarahkhan20/GitZen"
}